#!/usr/local/bin/cbsd
#v11.0.10
MYARG=""
MYOPTARG="jname"
MYDESC="Return jail ID in output and jail existance as error code (0: no jail, 1: jail exist)"
CBSDMODULE="jail"

. ${subrdir}/nc.subr
. ${strings}
. ${cbsdinit}

[ -z "${1}" ] && exit 0
[ -z "${jname}" ] && jname="${1}"

. ${subrdir}/rcconf.subr
[ $? -eq 1 ] && exit 0

# jail exist
echo "${myjid}"

exit 1

#!/usr/local/bin/cbsd
#v13.0.8
MYARG=""
MYOPTARG="ver"
MYDESC="Pull CBSD images from registry"
CBSDMODULE="sys"
ADDHELP="\

${H3_COLOR}Description${N0_COLOR}:

${H3_COLOR}Options${N0_COLOR}:


${H3_COLOR}Examples${N0_COLOR}:


"

. ${subrdir}/nc.subr
. ${system}
. ${strings}
. ${tools}

. ${cbsdinit}

cbsdsqlrw images "SELECT name,path,source,created,emulator,size FROM images"

exit 0


#!/usr/local/bin/cbsd
#v10.0.3
MYARG=""
MYOPTARG=""
MYDESC="Show cbsd history command"
ADDHELP="Use cbsd history [N] for output only last N commands\n"
EXTHELP="wf_history"

TAIL_OPT=""

if [ "$1" = "--help" ]; then
	${ECHO} ${MYDESC}
	${ECHO} ${ADDHELP}
	exit
elif [ -n "${1}" ]; then
	TAIL_OPT="-n${1}"
fi

[ -r "${workdir}/.cbsd_history" ] && tail ${TAIL_OPT} ${workdir}/.cbsd_history

